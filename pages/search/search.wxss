/* pages/search/search.wxss */
.all{
  background: #ededed;
}
.search{
  padding:5px ;
}
.search input{
  width: 98%;
  height: 56rpx;
  border: 1rpx #c66 solid;
  border-radius: 8px;
  background: #fff;
  box-sizing: border-box;
  padding-left: 70rpx;

}
.search image{
  position: absolute;
  top: 25rpx;
  left: 30rpx;
  background-size: 100%;
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}
.text{
  height: 20px;
  text-align: center;
  font-size: 13px;
  line-height: 20px;
  color: rgb(136, 134, 134);
}
.list-item{
  height: 60rpx;
  line-height: 60rpx;
  background: #fff;
  border-bottom: 1px #ededed solid;
}
.list-item text{
  font-size: 15px;
  margin:auto 20rpx;
}
.number{
  color: red;
}